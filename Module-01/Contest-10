**** P ****

void solve() {

	int a,b ;
	cin >> a >> b;

	if(a > b){
		cout << 0 << endl;
	}
	else{
		cout << b-a+1 << endl;
	}


}	


**** Q ****


void solve() {

	int x ;
	cin >> x ; 
	if(x == 0){
		cout << 1 << endl;
	}
	else if(x == 1){
		cout << 0 << endl;
	}


}	


