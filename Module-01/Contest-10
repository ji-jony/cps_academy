//#include<bits/stdc++.h>
#include<iostream>
#include<string>
#include<cmath>
using namespace std;

#define testcase int TT; cin >> TT; for (int tc = 0; tc < TT; tc++)
#define endl "\n"
#define PI (2.0 * acos(0.0))
typedef long long ll;


void solve() {

	int a,b ;
	cin >> a >> b;

	if(a > b){
		cout << 0 << endl;
	}
	else{
		cout << b-a+1 << endl;
	}


}	


int main() {

	ios_base::sync_with_stdio(false);
	cin.tie(NULL);

	//testcase{
		//cout << "Case " << tc+1 << " : " ;
		solve();
	//}

	return 0;
}


