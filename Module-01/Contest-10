**** D ****

void solve() {

	int a, b;
	cin >> a >> b;

	if(((a == 2 || b == 2) && a+b > 3)|| (a%2==0 && b%2==0)){
		cout << "Yes" << endl;
	}
	else {
		cout << "No" << endl;
	}
	
	


}	


**** E ****

int x,y,k;
	cin >> x >> y >> k;

	int z = x+k;

	if(x < y){
		if(z < y){
			cout << z + (2 * (y - z)) << endl;
		}
		else {
			cout << y << endl;
		}
	}
	else if(x > y){
		cout << x << endl;
	}


**** F ****

void solve() {

	string pin;
	cin >> pin;
	int digit[4] ;

	for(int i = 0; i < 4; i++){
		digit[i] = pin[i] - '0' ;
		if(digit[i] == 0){
			digit[i] = 10;
		}
	}

	int a = digit[0] ;
	int b = abs(digit[0] - digit[1]);
	int c = abs(digit[1] - digit[2]);
	int d = abs(digit[2] - digit[3]);

	int res = a+b+c+d+3;

	cout << res << endl;
	


}	



**** G ****

int n, sum = 0;
	cin >> n;
	int item[n-1] ;

	for(int i = 0; i < n-1; i++){
		cin >> item[i] ;
		sum += item[i];
	}

	if(sum < 0){
		cout << abs(sum) << endl;
	}
	else if(sum > 0){
		cout << -sum << endl;
	}
	else {
		cout << 0 << endl;
	}


**** H ****

int x,y,k;
	cin >> x >> y >> k;

	int z = x+k;

	if(x < y){
		if(z < y){
			cout << z + (2 * (y - z)) << endl;
		}
		else {
			cout << y << endl;
		}
	}
	else if(x > y){
		cout << x << endl;
	}


**** I ****
**** J ****

int n, sum = 0;
	cin >> n;
	int item[n] ;

	for(int i = 0; i < n; i++){
		cin >> item[i] ;
		if((i+1) == item[i]){
			sum++;
		}
	}

	if(sum % 2 != 0){
		cout << (sum / 2)+1 << endl;
	}
	else if(sum % 2 == 0){
		cout << sum/2 << endl;
	}
	else {
		cout << 0 << endl;
	}


**** K ****
**** L ****

void solve() {

	int n; 
	cin >> n;

	int x = n % 10 ;

	if(x == 1 || x ==6){
		cout << n - 1 << endl;
	}
	else if(x == 2 || x == 7){
		cout << n - 2 << endl;
	}
	else if(x == 3 || x == 8){
		cout << n + 2 << endl;
	}
	else if(x == 4 || x == 9){
		cout << n + 1 << endl;
	}
	else if (x == 0 || x == 5){
		cout << n << endl;
	}

	

}	


