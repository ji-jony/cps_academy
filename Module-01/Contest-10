**** A ****

//#include<bits/stdc++.h>
#include<iostream>
using namespace std;

#define testcase int TT; cin >> TT; for (int tc = 0; tc < TT; tc++)
#define endl "\n"
#define PI (2.0 * acos(0.0))
typedef long long ll;


void solve() {
	int n,m,p;
	int cnt = 0;
	cin >> n >> m >> p;

	if(m > n){
		cout << 0 << endl ;
	}
	else {
		cnt++;

		for(int i = 1; ; i++){
			if((m+(i*p)) <= n){
				cnt++;
			}
			else{
				break;
			}
		}
		cout << cnt << endl;
	}
}	


int main() {

	ios_base::sync_with_stdio(false);
	cin.tie(NULL);

	//testcase{
		//cout << "Case " << tc+1 << " : " ;
		solve();
	//}

	return 0;
}


**** B ****
//#include<bits/stdc++.h>
#include<iostream>
using namespace std;

#define testcase int TT; cin >> TT; for (int tc = 0; tc < TT; tc++)
#define endl "\n"
#define PI (2.0 * acos(0.0))
typedef long long ll;


void solve() {
	int x1,y1,x2,y2,x3,y3,x4,y4;
	cin >> x1 >> y1 ;
	cin >> x2 >> y2 ;
	cin >> x3 >> y3 ;
	cin >> x4 >> y4 ;

	int x = x2 - x1 ;
	int y = y2 - y1 ;

	if(x != 0){
		cout << x*x << endl;
	}
	else {
		cout << y*y << endl;
	}

}	


int main() {

	ios_base::sync_with_stdio(false);
	cin.tie(NULL);

	testcase{
		//cout << "Case " << tc+1 << " : " ;
		solve();
	}

	return 0;
}

**** C ****
**** D ****
**** E ****
**** F ****

//#include<bits/stdc++.h>
#include<iostream>
#include<string>
#include<cmath>
using namespace std;

#define testcase int TT; cin >> TT; for (int tc = 0; tc < TT; tc++)
#define endl "\n"
#define PI (2.0 * acos(0.0))
typedef long long ll;


void solve() {

	string pin;
	cin >> pin;
	int digit[4] ;

	for(int i = 0; i < 4; i++){
		digit[i] = pin[i] - '0' ;
		if(digit[i] == 0){
			digit[i] = 10;
		}
	}

	int a = digit[0] ;
	int b = abs(digit[0] - digit[1]);
	int c = abs(digit[1] - digit[2]);
	int d = abs(digit[2] - digit[3]);

	int res = a+b+c+d+3;

	cout << res << endl;
	


}	


int main() {

	ios_base::sync_with_stdio(false);
	cin.tie(NULL);

	testcase{
		//cout << "Case " << tc+1 << " : " ;
		solve();
	}

	return 0;
}

