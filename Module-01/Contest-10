	int res = (7-a)+(7-b)+(7-c);

	cout << res << endl;


}	

**** T ****

void solve() {

	int a,b;
	cin >> a >> b ;

	double c = (double)b/100 ;

	double res = a*c ;

	cout << res << endl ;


}	


**** U ****

void solve() {

	int n;
	cin >> n;

	if(n % 2 == 0){
		cout << n / 2 << endl ;
	}
	else {
		cout << (n/2)+1 << endl;
	}

}	


