**** W ****

void solve() {

	int a,b;
	cin >> a >> b;

	int res = (2 * a + 100) - b ;
	cout << res << endl;

}	


**** X ****

void solve() {

	int a, b; 
	cin >> a;
	cin >> b;
	
	cout << 6 - (a + b) << endl;;

}	


**** AB ****

void solve() {

	int n; 
	cin >> n;

	int h = n / 3600;
	int m = (n % 3600) / 60 ; 
	int s = n - ((h*3600) + (m*60)) ;

	cout << h << ":" << m << ":" << s << endl ;	

}	




















