#include<bits/stdc++.h>
//#include<iostream>
//#include<string>
//#include<cmath>
using namespace std;

#define testcase int TT; cin >> TT; for (int tc = 0; tc < TT; tc++)
#define endl "\n"
#define PI (2.0 * acos(0.0))
typedef long long ll;


/// Instead of Build in Function Ceil, for no data loss ;
/*int ceil(int a, int b) {
	return (a + b - 1) / b;
}*/


void solve() {

	int n;
	cin >> n;

	if(n % 2 == 0){
		cout << n / 2 << endl ;
	}
	else {
		cout << (n/2)+1 << endl;
	}

}	


int main() {

	ios_base::sync_with_stdio(false);
	cin.tie(NULL);

	//testcase{
		//cout << "Case " << tc+1 << " : " ;
		solve();
	//}

	return 0;
}

