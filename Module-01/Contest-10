///// Maths /////


**** A ****

void solve() {
	int n,m,p;
	int cnt = 0;
	cin >> n >> m >> p;

	if(m > n){
		cout << 0 << endl ;
	}
	else {
		cnt++;

		for(int i = 1; ; i++){
			if((m+(i*p)) <= n){
				cnt++;
			}
			else{
				break;
			}
		}
		cout << cnt << endl;
	}
}	



**** B ****

void solve() {
	int x1,y1,x2,y2,x3,y3,x4,y4;
	cin >> x1 >> y1 ;
	cin >> x2 >> y2 ;
	cin >> x3 >> y3 ;
	cin >> x4 >> y4 ;

	int x = x2 - x1 ;
	int y = y2 - y1 ;

	if(x != 0){
		cout << x*x << endl;
	}
	else {
		cout << y*y << endl;
	}

}	


