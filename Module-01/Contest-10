#include<cmath>
using namespace std;

#define testcase int TT; cin >> TT; for (int tc = 0; tc < TT; tc++)
#define endl "\n"
#define PI (2.0 * acos(0.0))
typedef long long ll;


void solve() {

	int a,b,c ;
	cin >> a >> b >> c;

	int res = (7-a)+(7-b)+(7-c);

	cout << res << endl;


}	


int main() {

	ios_base::sync_with_stdio(false);
	cin.tie(NULL);

	//testcase{
		//cout << "Case " << tc+1 << " : " ;
		solve();
	//}

	return 0;
