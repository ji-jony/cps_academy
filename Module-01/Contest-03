    int n;
	cin >> n;

	cout << 24 * n * 1000 << endl;

**** P ****

    int x,y,z;
	cin >> x >> y >> z ;

	if((x * y) <= (z * 24 * 60)){
		cout << "YES" << endl;
	}
	else {
		cout << "NO" << endl;
	}

**** Q ****

int n,k;
	cin >> n >> k ;

	int res = (n / (k+1)) ;
	int rem = n - (res * (k+1)) ;

	cout << res + rem << endl; 


**** R ****

int x, y, z;
	cin >> x >> y >> z;	

	int res = (x*y)/2 ;

	if( z > res){
		cout << "YES" << endl;
	}
	else {
		cout << "NO" << endl;
	}




