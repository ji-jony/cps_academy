    int t,n,x;
    scanf("%d",&t);
    while(t--){
        scanf("%d %d",&n,&x);
        printf("%d\n",n-x );
    }
    return 0;
}

**** J ****

#include<stdio.h>

int main() {
    int t,n,x;
    scanf("%d",&t);
    while(t--){
        scanf("%d %d",&n,&x);
        if(n<=x){
            printf("YES\n");
        }
        else {
            printf("NO\n");
        }
    }
    return 0;
}

**** L ****

#include<stdio.h>
int main() {
    int t,x;
    scanf("%d",&t);
    while(t--){
        scanf("%d",&x);
        if(x<1400){
            printf("Division 4\n");
        }
        else if(x<1600){
            printf("Division 3\n");
        }
        else if(x<1900){
            printf("Division 2\n");
        }
        else {
            printf("Division 1\n");
        }
        
    }
    return 0;
}

**** M ****

string ball[6] ;
	for(int i = 0; i < 6; i++){
		cin >> ball[i] ;
	}
	bool hat = false ;

	for(int i = 0; i < 4; i++){
		if(ball[i] == "W" && ball[i+1] == "W" && ball[i+2] == "W"){
			hat = true ;
		}
	}
	if(hat){
		cout << "YES" << endl;
	}
	else {
		cout << "NO" << endl;
	}

**** N ****

    int x,n;
	cin >> x >> n;

	if( x*100 > n){
		cout << 0 << endl;
	}
	else {
		int res = n - (x*100) ;
		if(res % 100 == 0){
			cout << res / 100 << endl;
		}
		else {
			cout << (res / 100) + 1 << endl;
		}
	}

