<!---- Loops ---->

**** A ****

#include<stdio.h>
int main() {
    int x, t ;
    scanf("%d",&t);
    while(t--){
        scanf("%d",&x);
        printf("%d\n",x-10 );
    } 
    return 0;
}

**** B ****

#include<stdio.h>
int main() {
    int n,m,t ;
    scanf("%d",&t);
    while(t--){
        scanf("%d %d",&n,&m);
        if(m>=n){
            printf("0\n");
        }
        else {
            printf("%d\n",n-m);
        }
    } 
    return 0;
}

**** C ****

#include<stdio.h>
int main() {
    int x,y,t ;
    scanf("%d",&t);
    while(t--){
        scanf("%d %d",&x,&y);
        printf("%d\n",y-x );
    } 
    return 0;
}

**** D ****

#include<stdio.h>
int main() {
    int t,k;
    scanf("%d",&t);
    while(t--){
        scanf("%d",&k);
        int cnt = 0;
        for(int i = 1 ; ; i++ ){
            if(i % 3 == 0 || i % 10 == 3){
                continue;
            }
            cnt++;
            if(cnt == k ){
                printf("%d\n",i );
                break;
            }
        }
    }

    return 0;
}

**** E ****

#include<stdio.h>

int main() {
    int t,x;
    scanf("%d",&t);
    while(t--){
        scanf("%d",&x);
        if(x > 24){
            printf("YES\n");
        }
        else {
            printf("NO\n");
        }
    }
    return 0;
}

**** F ****

#include<stdio.h
int main() {
    int n;
    scanf("%d",&n);
    printf("L");
    for(int i = 0; i < n; i++){
        printf("o");
    }
    printf("ng\n");
    return 0;
}
