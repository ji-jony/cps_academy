}


**** M ****

#include<iostream>
#include<iomanip>
#include<cmath>
using namespace std;

int main() {

    int n,m;
    cin >> n >> m ;
    int x,y;
    cin >> x >> y ;

    cout << (x*n)+(y*m) << endl;
    return 0;
}


