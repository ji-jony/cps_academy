    return 0;
}


**** D ****

#include<iostream>
#include<iomanip>
using namespace std;

int main() {

    int a,b;

    cin >> a >> b ;

    cout << 7 - (a + b) << endl ;

    return 0;
}

**** E ****

#include<iostream>
#include<iomanip>
using namespace std;

int main() {

    int a,b;

    cin >> a >> b ;

    cout << a/b << " " << a%b << " "<< fixed << setprecision(5) << (double)a/b<< endl ;

    return 0;
}

**** F ****

#include<iostream>
#include<iomanip>
using namespace std;

int main() {

    double r;
    double pi = 3.141592653589793238462643383279502884197;

    cin >> r ;

    cout << fixed << setprecision(6) << pi*r*r << " " << 2*r*pi << endl;

    return 0;
}


**** G ****

#include<iostream>
#include<iomanip>
using namespace std;

int main() {

    int abc;
    cin >> abc;
    int a = abc / 100;
    int b = (abc % 100)/10;
    int c = abc % 10;
    int f1 = a*100 + b*10 + c;
    int f2 = a + b*100 + c*10;
    int f3 = a*10 + b + c*100;

    cout << f1+f2+f3 << endl ;



    return 0;
}


**** H ****

#include<iostream>
#include<iomanip>
using namespace std;

int main() {

    long long int x,y;
    cin >> x >> y;
    long long int res = x/y;

    if(x%y == 0) {
        cout << res << endl ;
    }
    else {
        cout << res+1 << endl ;
    }



    return 0;
}


**** I ****

#include<iostream>
#include<iomanip>
#include<cmath>
using namespace std;

int main() {

    int x,y;
    cin >> x >> y;
    int res = pow(x,y);
    cout << res << endl;
    return 0;
}


**** J ****

#include<iostream>
#include<iomanip>
#include<cmath>
using namespace std;

int main() {

    int x,y;
    cin >> x >> y;
    int res = (x+y)*10 +1;
    cout << res << endl;
    return 0;
}

