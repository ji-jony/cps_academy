**** R ****

#include<iostream>
#include<iomanip>
#include<cmath>
#include<bitset>
using namespace std;

int main() {

    int a , b ;

    cin >> a >> b;

    int res = pow(a,b) + pow(b,a);

    cout << res << endl ;


    return 0;
}









