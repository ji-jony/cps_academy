///// Data types, Variables, Operators /////


**** A ****

#include<iostream>
using namespace std;
int main() {

    int n;
    cin >> n;

    if(n%100 == 0){
        cout << "K00" << endl ;
    }
    else if(n%100 >= 1 && n%100 <= 9 ){
        cout << "K0" << n%10 << endl ;
    }
    else {
        cout << "K" << n%100 << endl ;
    }

    return 0;
}

**** B ****

#include<iostream>
#include<iomanip>
using namespace std;

int main() {

    int a, b;

    cin >> a >> b;

    cout << a+b << endl ;

    return 0;
}


