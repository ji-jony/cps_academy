**** O ****

#include<iostream>
#include<iomanip>
#include<cmath>
using namespace std;

int main() {

    int bn[128], n ;
    int cnt = 0;

    cin >> n ;

    int i = 0;

    while(n > 0){
        bn[i] = n % 2 ;
        n = n/2 ;
        i++;
    }
    for(int j = 0; j <= i-1 ; j++){
        if(bn[j] == 0){
            cnt++;
        }
        if(bn[j] == 1){
            break;
        }
    }
    cout << cnt << endl;

    return 0;
}


